{
  "expo": {
    "name": "FinZen AI",
    "slug": "finzenai-mobile",
    "version": "1.0.0",
    "scheme": "finzenai",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.jl.alonso.finzenaimobile",
      "buildNumber": "7",
      "associatedDomains": [
        "applinks:finzenai-backend-production.up.railway.app"
      ],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSMicrophoneUsageDescription": "Esta app necesita acceso al micrófono para permitirte hablar con Zenio, tu asistente financiero.",
        "NSSpeechRecognitionUsageDescription": "Esta app necesita acceso al reconocimiento de voz para transcribir tus consultas financieras a Zenio.",
        "NSFaceIDUsageDescription": "Usamos Face ID para que puedas acceder rápida y seguramente a tu cuenta de FinZen AI sin necesidad de escribir tu contraseña cada vez.",
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.jl.alonso.finzenaimobile",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "RECORD_AUDIO",
        "INTERNET",
        "USE_BIOMETRIC",
        "USE_FINGERPRINT",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "90a0c5a6-5410-4eea-bcb7-afc42092cc0c"
      },
      "API_URL": "${API_URL}"
    },
    "owner": "jl.alonso",
    "plugins": [
      "expo-secure-store",
      "expo-asset",
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#2563EB",
          "sounds": [],
          "defaultChannel": "default"
        }
      ]
    ]
  }
}
